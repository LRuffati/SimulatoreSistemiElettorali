data_source County {election}:
	totals result: source.election.["candidato", "voti_cand"]

district District:
	subsections County counties {
		map res: func.result
	}

	totals result: subs.counties.res {
		key "candidato"
		sum "voti_cand" -> "voti"
	}

	direct_election uninominale {
		seats 1
		data {func.result}
		criteria commons.FPTP
		kwargs {candidate:"candidato", votes:"voti_cand"} 
		#Passa di default "data" e "self"
		electable Candidato #la classe che elegge
	}

electable Candidato:
	criteria Accept
